# Production Environment Variables
# Copy this file and update with your production values

# Azure AD Configuration (Production)
AZURE_AD_CLIENT_ID=your_production_client_id
AZURE_AD_CLIENT_SECRET=your_production_client_secret
AZURE_AD_TENANT_ID=your_tenant_id
AZURE_AD_REDIRECT_URI=https://your-domain.com/api/auth/azure/callback

# Supabase Production
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_production_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_production_service_role_key

# Webhook Secrets (Production - use strong random secrets)
# Generate with: openssl rand -base64 32
WEBHOOK_SECRET_UPTIMEROBOT=production_secret_uptimerobot
WEBHOOK_SECRET_FILEMAKER=production_secret_filemaker
WEBHOOK_SECRET_BACKUP=production_secret_backup
WEBHOOK_SECRET_AWS_S3=production_secret_aws_s3

# Session Configuration (Production - use strong random secret)
# Generate with: openssl rand -base64 32
SESSION_SECRET=production_session_secret_use_strong_random_string
SESSION_COOKIE_NAME=server_monitor_session
